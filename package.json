    "babel-jest": "^30.2.0",
    "eslint": "^10.0.2",
    "eslint-plugin-jest": "^29.15.0",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0"
